kubectl-aliases-help() {
    cat <<EOF
A bunch of convenient shell aliases for kubectl.

Examples:
    k - kubectl
    kg - kubectl get
    kgpo - kubectl get pod

    ksysgpo - kubectl --namespace=kube-system get pod

    kgsvc - kubectl get service
    kgdep - kubectl get deployment

    kgpooyaml - kubectl get pods -o yaml

    kgdbl dbid=deadbeef

Commands:
    g = get
    d = describe
    ex = exec -i -t
    lo = logs
    lof = logs -f
    lofp = logs -f -p

Resources
    po = pods
    dep = deployment
    sts = statefulsets
    ds = daemonset
    rs = replicaset
    svc = service
    pvc = persistentvolumeclaim
    pv = persistentvolume
    sc = storageclass
    sa = serviceaccount
    ro = role
    rob = rolebinding
    cro = clusterrole
    crob = clusterrolebinding
    pdb = poddisruptionbudget
    cm = configmap
    sec = secret
    jo = job
    cjo = cronjob
    crd = customresourcedefinition
    end = endpoint
    ev = event
    ev = event
    db = neo4jdatabase
    cn = canary
    fl = canaryflock
    no = nodes
    ns = namespaces

Arguments:
    oyaml = -o=yaml
    owide = -o=wide
    ojson = -o=json

Positional Arguments:
    l = -l
    n = --namespace
EOF
}
